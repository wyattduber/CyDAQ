<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="CyDAQ_standalone"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
CyDAQ_standalone
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="CyDAQ_standalone">
        <sdx:configuration sdx:name="CyDAQ_standalone"
                           sdx:displayName="CyDAQ_standalone"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>CyDAQ_standalone</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="CyDAQ_standalone/boot/CyDAQ_standalone.bif"
                            sdx:readme="CyDAQ_standalone/boot/generic.readme"
                            sdx:qemuArguments="CyDAQ_standalone/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="standalone_domain"
                            sdx:bif="CyDAQ_standalone/boot/CyDAQ_standalone.bif"
                            sdx:readme="CyDAQ_standalone/boot/generic.readme"
                            sdx:qemuArguments="CyDAQ_standalone/standalone_domain/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
                                sdx:displayName="standalone on ps7_cortexa9_1"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_1">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="CyDAQ_standalone/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="CyDAQ_standalone/standalone_domain/bspinclude/include"
                        sdx:bspConfig="CyDAQ_standalone/standalone_domain/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
